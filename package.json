{
  "name": "@yassilah/nuxt-crawler",
  "version": "0.0.1",
  "description": "Automatic crawler & search for Nuxt SSG",
  "repository": "git@github.com:yassilah/nuxt-crawler.git",
  "author": "Yasser Lahbibi <yasser.lahbibi@apenhet.com>",
  "license": "MIT",
  "keywords": [
    "nuxt",
    "algolia",
    "search",
    "crawler",
    "ssg"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    },
    "./dist/*": "./dist/*"
  },
  "main": "./dist/module.cjs",
  "types": "./dist/module.d.ts",
  "files": [
    "dist/module.cjs",
    "dist/module.d.ts",
    "dist/module.mjs"
  ],
  "scripts": {
    "build": "unbuild",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --commit-path . -l @yassipad/nuxt-crawler -r 1"
  },
  "dependencies": {
    "@nuxt/kit": "^0.8.1-edge",
    "algoliasearch": "latest",
    "metadata-scraper": "^0.2.44",
    "vue-demi": "*"
  },
  "devDependencies": {
    "@apenhet/eslint-config": "latest",
    "@nuxt/types": "latest",
    "conventional-changelog": "^3.1.24",
    "conventional-changelog-cli": "^2.1.1",
    "unbuild": "latest"
  },
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": "@apenhet"
  }
}
